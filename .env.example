# Multi-Schema Multi-Tenancy (db/ folder)
DATABASE_URL=postgresql://user:password@localhost:5432/database_name

# Row-Level Security Multi-Tenancy (db-rls/ folder) - Neon PostgreSQL
# Admin connection - for migrations (bypasses RLS, uses Neon owner credentials)
DATABASE_URL_ADMIN=postgresql://neondb_owner:password@ep-xxx-xxx.region.aws.neon.tech/neondb?sslmode=require

# App connection - for application queries (respects RLS, uses app_owner role)
DATABASE_URL_APP=postgresql://app_owner:password@ep-xxx-xxx.region.aws.neon.tech/neondb?sslmode=require